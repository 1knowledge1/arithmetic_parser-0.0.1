#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    char op;

    cin >> a >> op >> b;

    switch (op) {
        case '+':  cout <<a+b;
            break;
        case '-': cout <<a-b;
            break;
        case '*':  cout <<a*b;
            break;
        case '/': cout <<(double)a / b;
            break;
        case '%':  cout << a % b;
            break;
        case '&':   cout  <<(a&b);
            break;
        case '|':   cout  <<(a|b);
            break;
        case '^':   cout  <<(a^b);
            break;
        case '<':  c=a<<b;
                    cout  <<c;
            break;
        case '>': cout <<(a>>b);
            break;
        default: cout<<"ERROR!";

        }
    return 0;
}
